<%= erb :generic_header %>

<body>
  <%= erb :nav %>
  <div class="container">
    <h1>RESULTS!!!</h1>
    <% if ENV["UPLOAD_FILES_TO_AWS_S3"] %>
      Note: These emojis are saved for a limited time only. Make sure to download the ones you want to keep!
      <br>
    <% end %>
    <% @result.each_with_index do |r,i| %>
      <div class="output-container">
        <img class="output__content-image" src="<%= "data:image/gif;base64,#{r}" %>">
        <%= erb :result_actions, locals: {file: r} %>
      </div>
      <br>
      <br>
    <% end %>
  </div>
</body>
